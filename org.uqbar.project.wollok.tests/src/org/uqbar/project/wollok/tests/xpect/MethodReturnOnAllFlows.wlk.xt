/* XPECT_SETUP org.uqbar.project.wollok.tests.xpect.WollokXPectTest END_SETUP */

class A {
	
	method foo() = "Foo"
	
	method getFoo() { return "Foo" }
	
	method getBar() { 
		if (self.getFoo() == "Foo")  
			return "Foo"
		else
			return "Bar"
	}
	
	// XPECT errors --> "Must return a value on every possible flow" at "getZoo"
	method getZoo() { 
		if (self.getFoo() == "Foo")  
			return "Foo"
	}
	
	// XPECT errors --> "Must return a value on every possible flow" at "getZoo2"
	method getZoo2() { 
		if (self.getFoo() == "Foo")  
			return "Foo"
		
		var a = 2
		a += 1
	}
	
	// XPECT errors --> "Must return a value on every possible flow" at "getXoo"
	method getXoo() { 
		if (self.getFoo() == "Foo")  
			return "Foo"
		else
			"Bar"
	}
	
	// XPECT errors --> "Must return a value on every possible flow" at "getXoo2"
	method getXoo2() { 
		if (self.getFoo() == "Foo") {
			var a = 23
			a += 2
			return a
		}
		else
			"Bar"
	}
	
	// XPECT errors --> "Must return a value on every possible flow" at "getXoo3"
	method getXoo3() { 
		if (self.getFoo() == "Foo") {
			"Foo"
		}
		else {
			return "Bar"
		}
	}
	
	method getThrowingException() {
		if (self.getFoo() == "Foo") {
			throw new Exception() 
		}
		else {
			return "Foo"
		}
	}
	
	method getThrowingExceptionAtIf() {
		if (self.getFoo() == "Foo") {
			throw new Exception() 
		}
		var a = 23
		a += 2
		return "Foo"
	}
	
	method getThrowingExceptionAtBody() {
		if (self.getFoo() == "Foo") {
			return "Foo" 
		}
		var a = 23
		a += 2
		throw new Exception()
	}

}
